<idea-plugin>
    <id>org.thesis.thesis-plugin</id>
    <name>Security Scanner</name>
    <vendor email="erik.novysedlak@student.tuke.sk" url="https://fei.tuke.sk">TUKE</vendor>
    <description><![CDATA[
        Scan your code for security threats!<br>
        You can scan your code for security threats and get a report of the results.<br>
    ]]></description>
    <depends>com.intellij.modules.platform</depends>
    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Security Scanner" secondary="true" icon="AllIcons.Toolwindows.ToolWindowFind" anchor="right"
                    factoryClass="org.tuke.thesis.ToolWindow"/>
    </extensions>
    <resource-bundle/>
</idea-plugin>
